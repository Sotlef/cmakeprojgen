macro (license __PACKAGE_LICENSE_NAME)
	cmake_parse_arguments (__PACKAGE_LICENSE
		"PUBLIC"
		"FILE"
		""
		${ARGN})
		
	list (APPEND @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_LICENSES ${__PACKAGE_LICENSE_FILE})
	
	if (__PACKAGE_LICENSE_PUBLIC)
		set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_INSTALLER_FILE_LICENSE ${__PACKAGE_LICENSE_FILE})
		set (__PACKAGE_LICENSE_PUBLIC)
	endif ()
	set (__PACKAGE_LICENSE_FILE)
endmacro ()

macro (components)
	set (@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY)
endmacro ()

macro (endcomponents)
	if (@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY)
		
	endif ()
