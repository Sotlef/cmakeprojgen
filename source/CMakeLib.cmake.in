			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_DEPENDENCIES)
		else ()
			message (FATAL_ERROR "Unknown type '${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE}' specified for target '${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}'")
		endif ()
		
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES)
		
		if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_OUTPUTNAME)
