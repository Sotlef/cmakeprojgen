					else ()
                        set (COMPONENT_URL "vnd.sun.star.expand:\${ORIGIN}/unicom-${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MAJOR}/services/${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME}")
						configure_file (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_CATALOG}.in ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_BINDIR}/catalog/${COMPONENTFILENAME})
