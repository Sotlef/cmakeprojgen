macro (component __COMPONENT)
	cmake_parse_arguments (@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}
		"FEATURE"
		"TITLE;DESCRIPTION"
		"DEPENDS"
		${ARGN})
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_DESCRIPTION)
		set (@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_DESCRIPTION ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_TITLE})
	endif ()
	
	if (@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY)
		list (GET @CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY 0 @CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_GROUP)
		set (@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_GROUP_${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_GROUP} 1)
	endif ()
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${__COMPONENT}_FEATURE)
		list (APPEND @CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY ${__COMPONENT})
	endif ()
endmacro ()

macro (endcomponent)
	list (REMOVE_AT @CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_HIERARCHY 0)
endmacro ()

macro (installation @CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME)
	cmake_parse_arguments (__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION
		"EXPORT"
		"COMPONENT;TYPE;DEVELOPMENT;DESTINATION"
		""
		${ARGN})
	
	if (__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_TYPE)
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME)
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE)
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY)
		
		__determine_target_runtime__ (${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_TYPE})
		__determine_target_archive__ (${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_TYPE})
			
		if (__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DESTINATION)
			__determine_files_destination__ (${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_TYPE} ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DESTINATION})
		endif ()
	else ()
		message (FATAL_ERROR "Failed to determine command to install target ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}: target type is unknown")
	endif ()
	
	if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME OR @CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE OR @CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY)
		if (NOT __@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DEVELOPMENT)
			set (__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DEVELOPMENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_COMPONENT})
		endif ()

		if (__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_EXPORT)
			if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME AND @CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE)
				install (TARGETS ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}
						 EXPORT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets
						 RUNTIME DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME} COMPONENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_COMPONENT}
						 ARCHIVE DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE} COMPONENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DEVELOPMENT})
			elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME)
				install (TARGETS ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}
						 EXPORT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets
						 RUNTIME DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME} COMPONENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_COMPONENT})						 
			elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE)
				install (TARGETS ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}
						 EXPORT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets
						 ARCHIVE DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE} COMPONENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_DEVELOPMENT})
			elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY)
				install (TARGETS ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}
						 EXPORT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets
						 LIBRARY DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY} COMPONENT ${__@CMAKEPROJGEN_PROJECTNAME@_INSTALLATION_COMPONENT})
			endif ()
		else ()
