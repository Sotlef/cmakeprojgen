		if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Executable")
			add_executable (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME} ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES})
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Shared Library")
			add_library (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME} SHARED ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES})
