		endforeach ()
		
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_OUTPUT_PATH ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_BINDIR}/${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME})
		
		if (NOT ${CMAKE_CURRENT_SOURCE_DIR} STREQUAL ${CMAKE_SOURCE_DIR})
			foreach (var ${@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET})
				set (${var} ${${var}} PARENT_SCOPE)
			endforeach ()
		endif ()
	else ()
		message (WARNING "'${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}' has no source files. Target is ignored!")
	endif ()
