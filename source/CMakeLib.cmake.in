macro (files)
	cmake_parse_arguments (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES
		"RECURSE"
		"DIRECTORY;SYSTEM;PLATFORM"
		"PATTERN"
		${ARGN})
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_SYSTEM AND NOT @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PLATFORM)
		set (APPEND_FILES_TO_TARGET 1)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_SYSTEM)
		if (${CMAKE_SYSTEM_NAME} STREQUAL ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_SYSTEM})
			set (APPEND_FILES_TO_TARGET 1)
		endif ()
	elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PLATFORM AND ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PLATFORM})
		set (APPEND_FILES_TO_TARGET 1)
	endif ()
	
	if (APPEND_FILES_TO_TARGET)
		if (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR)
			set (TARGET_FILES_DIRECTORY ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR}/${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY})
		else ()
			set (TARGET_FILES_DIRECTORY ${CMAKE_SOURCE_DIR}/${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY})
		endif ()
		
		if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PATTERN)
			if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_RECURSE)
				file (GLOB_RECURSE TARGET_FILES RELATIVE ${TARGET_FILES_DIRECTORY} ${TARGET_FILES_DIRECTORY}/*)
							
				set (TARGET_FILES_DIRECTORIES)
				foreach (TARGET_FILE ${TARGET_FILES})
					if (IS_DIRECTORY (${TARGET_FILES_DIRECTORY}/${TARGET_FILE}))
						list (APPEND TARGET_FILES_DIRECTORIES ${TARGET_FILE})
					else ()
						get_filename_component (TARGET_DIRECTORY ${TARGET_FILE} DIRECTORY)
						if (TARGET_DIRECTORY)
							list (APPEND TARGET_FILES_DIRECTORIES ${TARGET_DIRECTORY})
						endif ()
					endif ()
				endforeach ()
				
				if (TARGET_FILES_DIRECTORIES)
					list (REMOVE_DUPLICATES TARGET_FILES_DIRECTORIES)
					list (SORT TARGET_FILES_DIRECTORIES)
									
					foreach (TARGET_DIRECTORY ${TARGET_FILES_DIRECTORIES})					
						string (REPLACE "/" "\\\\" TARGET_GROUP_NAME ${TARGET_DIRECTORY})
						group (${TARGET_GROUP_NAME})
							foreach (TARGET_PATTERN ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PATTERN})								
								file (GLOB TARGET_FILES RELATIVE ${TARGET_FILES_DIRECTORY} ${TARGET_FILES_DIRECTORY}/${TARGET_DIRECTORY}/${TARGET_PATTERN})
								
								if (TARGET_FILES)
									list (APPEND TARGET_FILES_TOTAL ${TARGET_FILES})
								endif ()
							endforeach ()
							
							if (TARGET_FILES_TOTAL)
								if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY)
									set (__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__)
									foreach (TARGET_FILE ${TARGET_FILES_TOTAL})
										list (APPEND __@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__ ${TARGET_FILES_DIRECTORY}/${TARGET_FILE})
									endforeach ()
									
									set (TARGET_FILES_TOTAL ${__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__})
								endif ()
								
								if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUPS)
									list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES ${TARGET_FILES_TOTAL})
								else ()
									list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES ${TARGET_FILES_TOTAL})
								endif ()
								set (TARGET_FILES_TOTAL)
							endif ()
						endgroup ()	
					endforeach ()
				endif ()
			endif ()
			
			foreach (TARGET_PATTERN ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_PATTERN})
				file (GLOB TARGET_FILES RELATIVE ${TARGET_FILES_DIRECTORY} ${TARGET_FILES_DIRECTORY}/${TARGET_PATTERN})
				
				if (TARGET_FILES)
					list (APPEND TARGET_FILES_TOTAL ${TARGET_FILES})
				endif ()
			endforeach ()
			
			if (TARGET_FILES_TOTAL)
				if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY)
					set (__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__)
					foreach (TARGET_FILE ${TARGET_FILES_TOTAL})
						list (APPEND __@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__ ${TARGET_FILES_DIRECTORY}/${TARGET_FILE})
					endforeach ()
					
					set (TARGET_FILES_TOTAL ${__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__})
				endif ()
				
				if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUPS)
					list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES ${TARGET_FILES_TOTAL})
				else ()
					list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES ${TARGET_FILES_TOTAL})
				endif ()
				set (TARGET_FILES_TOTAL)
			endif ()
		elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY)
			set (__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__)
			foreach (TARGET_FILE ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_UNPARSED_ARGUMENTS})
				list (APPEND __@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__ ${TARGET_FILES_DIRECTORY}/${TARGET_FILE})
			endforeach ()
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_DIRECTORY)
			files (${__@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES__})
		elseif (@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUPS)
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_UNPARSED_ARGUMENTS})
		else ()
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES_UNPARSED_ARGUMENTS})
		endif ()
		
		set (APPEND_FILES_TO_TARGET)
	endif ()
endmacro ()

macro (__apply_target_group_files)
	if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES)
		set (__TARGET_GROUP_NAME)
		foreach (__TARGET_GROUP ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUPS})
			if (__TARGET_GROUP_NAME)
				set (__TARGET_GROUP_NAME ${__TARGET_GROUP}\\${__TARGET_GROUP_NAME})
			else ()
				set (__TARGET_GROUP_NAME ${__TARGET_GROUP})
			endif ()
		endforeach ()
		
		if (__TARGET_GROUP_NAME)
			if (@CMAKEPROJGEN_PROJECTNAME@_INLINE_SCOPES)
				string (MAKE_C_IDENTIFIER ${__TARGET_GROUP_NAME} __TARGET_GROUP_C_NAME)
				set (@CMAKEPROJGEN_PROJECTNAME@_INLINED_GROUP_${__TARGET_GROUP_C_NAME} ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES})
				set (@CMAKEPROJGEN_PROJECTNAME@_INLINED_GROUP_NAME_${__TARGET_GROUP_C_NAME} ${__TARGET_GROUP_NAME})
				list (INSERT @CMAKEPROJGEN_PROJECTNAME@_INLINED_GROUPS 0 ${__TARGET_GROUP_C_NAME})
			else ()
				source_group (${__TARGET_GROUP_NAME} FILES ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES})
			endif ()
			
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_TARGET_FILES ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES})
		endif ()
		
		set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_GROUP_FILES)
	endif ()
endmacro ()

