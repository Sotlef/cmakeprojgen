			if (@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${@CMAKEPROJGEN_PROJECTNAME@_TARGET_COMPONENT}_TITLE)
				if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_DEVELOPMENT)
					installation (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}	EXPORT
						TYPE ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE}
						COMPONENT ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_COMPONENT}
						DEVELOPMENT ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_DEVELOPMENT})
				else ()
