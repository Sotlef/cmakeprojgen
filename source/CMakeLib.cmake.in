						TYPE ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE}
						COMPONENT ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_COMPONENT})				
				endif ()
				
				if (@CMAKEPROJGEN_PROJECTNAME@_TARGET_CATALOG)
					get_filename_component (COMPONENTFILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_CATALOG} NAME)
					
					if (WIN32 AND NOT UNIX)
						set (COMPONENT_URL "vnd.sun.star.expand:\${ORIGIN}/Components/${@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME}")
						configure_file (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_CATALOG}.in ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_BINDIR}/Catalog/${COMPONENTFILENAME})
