include (CMakeParseArguments)

if (WIN32)
    macro(get_WIN32_WINNT version)
        if (CMAKE_SYSTEM_VERSION)
            set(ver ${CMAKE_SYSTEM_VERSION})
            string(REGEX MATCH "^([0-9]+).([0-9])" ver ${ver})
            string(REGEX MATCH "^([0-9]+)" verMajor ${ver})
            # Check for Windows 10, b/c we'll need to convert to hex 'A'.
            if ("${verMajor}" MATCHES "10")
                set(verMajor "A")
                string(REGEX REPLACE "^([0-9]+)" ${verMajor} ver ${ver})
            endif ("${verMajor}" MATCHES "10")
            # Remove all remaining '.' characters.
            string(REPLACE "." "" ver ${ver})
            # Prepend each digit with a zero.
            string(REGEX REPLACE "([0-9A-Z])" "0\\1" ver ${ver})
            set(${version} "0x${ver}")
        endif(CMAKE_SYSTEM_VERSION)
    endmacro(get_WIN32_WINNT)

    get_WIN32_WINNT(ver)
    add_definitions(-D_WIN32_WINNT=${ver})
endif(WIN32)

macro (global)
	cmake_parse_arguments (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES
		""
		""
		"TARGET;INSTALLATION;PACKAGE"
		${ARGN})
	
	if (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET)
		if (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET)
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET})
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET})
		endif ()
		set (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET)
	endif ()

	if (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE)
		if (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE)
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE})
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE})
		endif ()
		set (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE)
	endif ()

	if (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION)
		if (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION)
			list (APPEND @CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION})
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION ${__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION})
		endif ()
		set (__@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION)
	endif ()
endmacro ()

macro (subdirectories)
	cmake_parse_arguments (__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE
		"OVERRIDE"
		"SRCDIR"
		""
		${ARGN})
	
	set (__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR_TEMP ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR})
	if ((NOT __@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OVERRIDE) AND __@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR)
		set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR ${__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR})
	endif ()
		
	foreach (dir ${__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_UNPARSED_ARGUMENTS})
		if (__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OVERRIDE AND __@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR)
			set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR ${__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR}/${dir})
		endif ()
		add_subdirectory (${dir})
	endforeach ()
	
	if (NOT ${CMAKE_CURRENT_SOURCE_DIR} STREQUAL ${CMAKE_SOURCE_DIR})
		foreach (var ${@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_TARGET} ${@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE} ${@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_INSTALLATION})
			set (${var} ${${var}} PARENT_SCOPE)
		endforeach ()
	endif ()
	set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR ${__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR_TEMP})
	
	set(__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR)
	set(__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OVERRIDE)
	set(__@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_UNPARSED_ARGUMENTS)
endmacro ()

macro (configure_cpack_generators)
	set (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR "None" CACHE STRING 
		"Select CPack generator for installer")
	set_property (CACHE @CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR PROPERTY STRINGS
		"None" "NSIS Installer" "Zip Archive" "Tar GZip Archive" "Self extracting Tar GZip Archive" "Tar BZip2 Archive" "Tar Archive")
		
	if (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "NSIS Installer")
		set (CPACK_GENERATOR NSIS)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "Zip Archive")
		set (CPACK_GENERATOR ZIP)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "Tar GZip Archive")
		set (CPACK_GENERATOR TGZ)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "Tar BZip2 Archive")
		set (CPACK_GENERATOR TBZ2)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "Self extracting Tar GZip Archive")
		set (CPACK_GENERATOR STGZ)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "Tar Archive")
		set (CPACK_GENERATOR TZ)
	elseif (@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR STREQUAL "None")
	else ()
		message (WARNING "Unknown CPack generator has been selected: ${@CMAKEPROJGEN_PROJECTNAME@_CPACK_GENERATOR}")
		message (WARNING "Select generator from the list")
	endif ()
endmacro ()

macro (__determine_target_filename__ @CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE)
	if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Executable")
		if (WIN32)
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.exe)
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME})
		endif ()
	elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Shared Library")
		if (WIN32)
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.dll)
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME lib${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.so)
		endif ()
	elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Static Library")
		if (WIN32)
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.lib)
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME lib${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.a)
		endif ()
	elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "C++ Component")
		if (WIN32)
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.dll)
		else ()
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_FILENAME lib${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}.so)
		endif ()
	else ()
		message (FATAL_ERROR "Unknown type '${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE}' specified for target '${@CMAKEPROJGEN_PROJECTNAME@_TARGET_NAME}'")
	endif ()
endmacro ()

macro (__determine_target_runtime__ @CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE)
	if (WIN32 AND NOT UNIX)
		if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Executable")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME "Bin")
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Shared Library")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME "Bin")
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "C++ Component")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME "Bin/Components")
		endif ()
	else ()
		if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Executable")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_RUNTIME "bin")
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Shared Library")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY "lib")
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "C++ Component")
            set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_LIBRARY "lib/unicom-${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MAJOR}/services")
		endif ()
	endif ()
endmacro ()

macro (__determine_target_archive__ @CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE)
	if (WIN32 AND NOT UNIX)
		if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Shared Library")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE "SDK/Lib")
		elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Static Library")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE "SDK/Lib")
		endif ()
	else ()
		if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Static Library")
			set (@CMAKEPROJGEN_PROJECTNAME@_TARGET_ARCHIVE "lib")
		endif ()
	endif ()
endmacro ()

macro (__determine_files_destination__ @CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE @CMAKEPROJGEN_PROJECTNAME@_TARGET_DESTINATION)
	if (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Directory")
		set (@CMAKEPROJGEN_PROJECTNAME@_DIRECTORY_DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_DESTINATION})
	elseif (${@CMAKEPROJGEN_PROJECTNAME@_TARGET_TYPE} STREQUAL "Files")
		set (@CMAKEPROJGEN_PROJECTNAME@_FILES_DESTINATION ${@CMAKEPROJGEN_PROJECTNAME@_TARGET_DESTINATION})
	else ()
		set (@CMAKEPROJGEN_PROJECTNAME@_DIRECTORY_DESTINATION)
		set (@CMAKEPROJGEN_PROJECTNAME@_FILES_DESTINATION)
	endif ()
endmacro ()

macro (package @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_NAME)
	cmake_parse_arguments (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE
		""
		"VERSION;URL_HELP;EXPORT_NAME;EXPORT_SRCDIR;OUTDIR"
		""
		${ARGN})
		
	project (${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_NAME})
	
	string (REPLACE "." ";" __package_version__ ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION})
	list (GET __package_version__ 0 @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MAJOR)
	list (GET __package_version__ 1 @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MINOR)
	list (GET __package_version__ 2 @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_PATCH)
	
	set_property (GLOBAL PROPERTY USE_FOLDERS ON)
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OUTDIR)
		set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OUTDIR ${CMAKE_BINARY_DIR}/output)
	endif ()
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_SRCDIR)
		set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_SRCDIR ${CMAKE_SOURCE_DIR}/cmake)
	endif ()
	
	if (NOT @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME)
		set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_NAME})
	endif ()
	
	set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_SRCDIR ${CMAKE_CURRENT_SOURCE_DIR})
	
	configure_cpack_generators ()
	
	global (
		PACKAGE
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_NAME
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MAJOR 
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MINOR 
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_PATCH
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_OUTDIR
		INSTALLATION
			@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_TARGETS_TO_EXPORT
			@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_USED
	)
endmacro ()

macro (endpackage)
	if (CPACK_GENERATOR AND @CMAKEPROJGEN_PROJECTNAME@_PACKAGE_TARGETS_TO_EXPORT)
		export (TARGETS ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_TARGETS_TO_EXPORT}
			FILE ${PROJECT_BINARY_DIR}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets.cmake)
		
		export (PACKAGE ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME})
		
		if (WIN32 AND NOT CYGWIN)
			set (INSTALL_CMAKE_DIR CMake)
		else ()
			set (INSTALL_CMAKE_DIR lib/CMake/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME})
		endif ()

		configure_file (${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_SRCDIR}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}ConfigVersion.cmake.in
			${PROJECT_BINARY_DIR}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}ConfigVersion.cmake @ONLY)
		configure_file (${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_SRCDIR}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Config.cmake.in
			${PROJECT_BINARY_DIR}${CMAKE_FILES_DIRECTORY}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Config.cmake @ONLY)
		install (FILES
			${PROJECT_BINARY_DIR}${CMAKE_FILES_DIRECTORY}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Config.cmake
			${PROJECT_BINARY_DIR}/${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}ConfigVersion.cmake
			DESTINATION ${INSTALL_CMAKE_DIR} COMPONENT devtools)
		install (EXPORT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}Targets
			DESTINATION ${INSTALL_CMAKE_DIR} COMPONENT devtools)
			
		set (CPACK_COMPONENTS_ALL ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_USED})
		
		set (CPACK_PACKAGE_NAME ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME})
		set (CPACK_PACKAGE_VENDOR ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_AUTHOR})
		set (CPACK_PACKAGE_VERSION ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION})
		set (CPACK_PACKAGE_VERSION_MAJOR ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MAJOR})
		set (CPACK_PACKAGE_VERSION_MINOR ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_MINOR})
		set (CPACK_PACKAGE_VERSION_PATCH ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION_PATCH})
		
		if (WIN32 AND NOT UNIX)
			set (CPACK_PACKAGE_INSTALL_DIRECTORY "${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME}\\\\${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_VERSION}")
		endif ()
		
		if (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_INSTALLER_FILE_LICENSE)
			set (CPACK_RESOURCE_FILE_LICENSE ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_INSTALLER_FILE_LICENSE})
			set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_INSTALLER_FILE_LICENSE)
		endif ()
		
		if (${CPACK_GENERATOR} STREQUAL NSIS)
			set (CPACK_NSIS_MODIFY_PATH ON)
			set (CPACK_NSIS_DISPLAY_NAME ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_EXPORT_NAME})
			if (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_CONTACT)
				set (CPACK_NSIS_CONTACT ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_CONTACT})
				set (@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_CONTACT)
			endif ()
		endif ()

		foreach (C ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENTS_USED})
			string (TOUPPER ${C} CUP)
			set (CPACK_COMPONENT_${CUP}_DISPLAY_NAME ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_TITLE})
			set (CPACK_COMPONENT_${CUP}_DESCRIPTION  ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_DESCRIPTION})
			set (CPACK_COMPONENT_${CUP}_DEPENDS      ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_DEPENDS})
			
			if (@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_GROUP)
				string (TOUPPER ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_GROUP} CGR)
				set (CPACK_COMPONENT_${CUP}_GROUP ${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_GROUP})
				set (CPACK_COMPONENT_GROUP_${CGR}_DISPLAY_NAME
						${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_GROUP}_TITLE})
				set (CPACK_COMPONENT_GROUP_${CGR}_DESCRIPTION
						${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${@CMAKEPROJGEN_PROJECTNAME@_COMPONENT_${C}_GROUP}_DESCRIPTION})
			endif ()
		endforeach ()
		
		include (CPack)

		if (NOT ${CMAKE_CURRENT_SOURCE_DIR} STREQUAL ${CMAKE_SOURCE_DIR})
			foreach (var ${@CMAKEPROJGEN_PROJECTNAME@_GLOBAL_VARIABLES_PACKAGE})
				set (${var} ${${var}} PARENT_SCOPE)
			endforeach ()
		endif ()
	# else ()
		# message ("CPack PACKAGE target will not be created:")
		# message ("CPACK_GENERATOR: ${CPACK_GENERATOR}")
		# message ("@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_TARGETS_TO_EXPORT: ${@CMAKEPROJGEN_PROJECTNAME@_PACKAGE_TARGETS_TO_EXPORT}")
	endif ()
endmacro ()

